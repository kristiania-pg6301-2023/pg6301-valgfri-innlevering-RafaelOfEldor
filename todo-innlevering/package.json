{
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/jest": "29.5.5",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0"
  },
  "dependencies": {
    "@babel/preset-typescript": "7.23.2",
    "@types/jest": "29.5.5",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@babel/plugin-transform-typescript": "7.22.15",
    "@types/prop-types": "15.7.8",
    "@types/scheduler": "0.16.4",
    "csstype": "3.1.2"
  },
  "scripts": {
    "dev": "concurrently npm:dev:client npm:dev:server",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "postinstall": "npm run install:client && npm run install:server",
    "install:client": "cd client && npm install --include=dev",
    "install:server": "cd server && npm install --include=dev",
    "build": "npm run build:client",
    "start": "cd server && npm start",
    "build:client": "cd client && npm run build",
    "check": "npm run check:prettier && npm run check:typescript && npm test",
    "prettier": "prettier --write .",
    "check:prettier": "prettier --check .",
    "check:typescript": "npm run check:typescript:client",
    "check:typescript:client": "cd client && npm run check:typescript",
    "test": "npm run test:client && npm run test:server",
    "test:client": "cd client && npm test",
    "test:server": "cd server && npm test"
  }
}

